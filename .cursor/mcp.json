{
    "mcpServers": {
      "supabase": {
        "command": "npx",
        "args": [
          "-y",
          "@supabase/mcp-server-supabase@latest",
          "--access-token",
          "[SUA_CHAVE_SUPABASE_AQUI]"
        ]
      },
      "cloudflare-observability": {
        "command": "npx",
        "args": [
          "mcp-remote", 
          "https://observability.mcp.cloudflare.com/sse"
        ]
      },
      "cloudflare-bindings": {
        "command": "npx",
        "args": [
          "mcp-remote", 
          "https://bindings.mcp.cloudflare.com/sse"
        ]
      }
    }
  }